config wifi-device  radio0
	option type     mac80211
	option channel  8
	option hwmode	11g
	option path	'platform/ar933x_wmac'
	option htmode	HT40
config wifi-iface
	option device   radio0
	option network  wan
#	option mode     ap
	option mode     sta
#	option ssid     JoySince
	option ssid     1437-7-1004
	option encryption psk2
	option key kunlong123456
	option bssid 00:ca:01:0f:00:f2
	option macaddr 00:ca:01:0f:00:01
